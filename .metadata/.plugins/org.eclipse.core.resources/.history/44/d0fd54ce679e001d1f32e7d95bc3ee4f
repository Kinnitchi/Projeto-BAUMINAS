)) {throw "Unidade de Faturamento n\u00E3o pode ser vazio.";}if ((form.getValue("TXT_UnidOrigem") == null || form.getValue("TXT_UnidOrigem") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Unidade de Origem - Licita\u00E7\u00F5es n\u00E3o pode ser vazio.";}if ((form.getValue("TXT_Produto") == null || form.getValue("TXT_Produto") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Produto n\u00E3o pode ser vazio.";}if ((form.getValue("txt_QuantTotal") == null || form.getValue("txt_QuantTotal") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Quantidade Licitada (em Ton) n\u00E3o pode ser vazio.";}if ((form.getValue("TXT_QtotalKg") == null || form.getValue("TXT_QtotalKg") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Quantidade Total (Kg) n\u00E3o pode ser vazio.";}if ((form.getValue("TXT_PFKg") == null || form.getValue("TXT_PFKg") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Pre\u00E7o Final (R$/Kg) n\u00E3o pode ser vazio.";}if ((form.getValue("TXT_CondPg") == null || form.getValue("TXT_CondPg") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Condi\u00E7\u00E3o de Pagamento n\u00E3o pode ser vazio.";}if ((form.getValue("Rd_PrazoEnt") == null || form.getValue("Rd_PrazoEnt") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Prazo de Entrega n\u00E3o pode ser vazio.";}if ((form.getValue("CMB_TipoFrete") == null || form.getValue("CMB_TipoFrete") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 &