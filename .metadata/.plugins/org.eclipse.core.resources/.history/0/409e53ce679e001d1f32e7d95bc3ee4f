function enableFields(form) {		var activity = getValue('WKNumState');if (activity == 4 || activity == 0) {form.setEnabled('txt_codFluig', false);form.setEnabled('TXT_CodProd', false);}if (activity == 21) {form.setEnabled('txt_codFluig', false);form.setEnabled('TXT_Cliente', false);form.setEnabled('TXT_CodCliente', false);form.setEnabled('TXT_CNPJ', false);form.setEnabled('TXT_NumPed', false);form.setEnabled('TXT_ContratoAta', false);form.setEnabled('TXT_EditEmpenho', false);form.setEnabled('DT_ContratoDe', false);form.setEnabled('Dt_ContratoAte', false);form.setEnabled('Rd_ZerarSd', false);form.setEnabled('CMB_UnidFaturam', false);form.setEnabled('TXT_UnidOrigem', false);form.setEnabled('TXT_Produto', false);form.setEnabled('txt_QuantTotal', false);form.setEnabled('TXT_QtotalKg', false);form.setEnabled('TXT_QTotalLt', false);form.setEnabled('TXT_PFKg', false);form.setEnabled('TXT_PFLt', false);form.setEnabled('TXT_CondPg', false);form.setEnabled('Rd_PrazoEnt', false);form.setEnabled('TXT_PrazoEntreg', false);form.setEnabled('CMB_TipoFrete', false);form.setEnabled('TXT_Valor', false);form.setEnabled('TXT_SERED', false);form.setEnabled('TXT_TipoEntrega', false);form.setEnabled('checkbox39opc0', false);form.setEnabled('checkbox39opc1', false);form.setEnabled('checkbox39opc2', false);form.setEnabled('checkbox39opc3', false);form.setEnabled('checkbox39opc4', false);form.setEnabled('checkbox39opc5', false);form.setEnabled('TXT_HrReb', false);form.setEnabled('TXT_ObsLogistic', false);form.setEnabled('TXT_ObsLogDist', false);form.setEnabled('Rd_Mangote', false);form.setEnabled('TXT_MangMetros', false);form.setEnabled('Rd_Bomba', false);form.setEnabled('TXT_Bomba', false);form.setEnabled('Rd_Engate', false);form.setEnabled('TXT_Polegadas', false);form.setEnabled('TXT_EspProd', false);form.setEnabled('TXT_LaudoEsp', false);form.setEnabled('Rd_DemEquip', false);form.setEnabled('TXT_EspEquip', false);}if (activity == 7) {form.setEnabled('TXT_Cliente', false);form.setEnabled('TXT_CodCliente', false);form.setEnabled('TXT_CNPJ', false);form.setEnabled('TXT_ContratoAta', false);form.setEnabled('TXT_EditEmpenho', false);form.setEnabled('DT_ContratoDe', false);form.setEnabled('Dt_ContratoAte', false);form.setEnabled('Rd_ZerarSd', false);form.setEnabled('CMB_UnidFaturam', false);form.setEnabled('TXT_UnidOrigem', false);form.setEnabled('TXT_Produto', false);form.setEnabled('txt_QuantTotal', false);form.setEnabled('Rd_DemEquip', false);form.setEnabled('TXT_QtotalKg', false);form.setEnabled('TXT_QTotalLt', false);form.setEnabled('TXT_PFKg', false);form.setEnabled('TXT_PFLt', false);form.setEnabled('TXT_CondPg', false);form.setEnabled('Rd_PrazoEnt', false);form.setEnabled('TXT_PrazoEntreg', false);form.setEnabled('CMB_TipoFrete', false);form.setEnabled('TXT_Valor', false);form.setEnabled('TXT_SERED', false);form.setEnabled('TXT_TipoEntrega', false);form.setEnabled('checkbox39opc0', false);form.setEnabled('checkbox39opc1', false);form.setEnabled('checkbox39opc2', false);form.setEnabled('checkbox39opc3', false);form.setEnabled('checkbox39opc4', false);form.setEnabled('checkbox39opc5', false);form.setEnabled('TXT_HrReb', false);form.setEnabled('TXT_ObsLogDist', false);form.setEnabled('Rd_Mangote', false);form.setEnabled('TXT_MangMetros', false);form.setEnabled('Rd_Bomba', false);form.setEnabled('TXT_Bomba', false);form.setEnabled('Rd_Engate', false);form.setEnabled('TXT_Polegadas', false);form.setEnabled('TXT_EspProd', false);form.setEnabled('TXT_LaudoEsp', false);form.setEnabled('TXT_EspEquip', false);form.setEnabled('txt_codFluig', false);form.setEnabled('TXT_CodProd', false);form.setEnabled('txt_unidOriDist', false);}if (activity == 15) {form.setEnabled('txt_codFluig', false);}}